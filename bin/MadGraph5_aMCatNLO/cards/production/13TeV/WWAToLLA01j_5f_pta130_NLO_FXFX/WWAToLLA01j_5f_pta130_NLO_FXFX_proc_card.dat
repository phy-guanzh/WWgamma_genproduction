import model loop_sm-no_b_mass 
 
define p = p b b~ 
define j = j b b~ 
 
define lep+ = e+ mu+ ta+ 
define lep- = e- mu- ta-
define nu = ve vm vt 
define nu~ = ve~ vm~ vt~
 
generate p p > lep+ lep- nu nu~ a [QCD] @0
add process p p > lep+ lep- nu nu~ j a [QCD] @1

output WWAToLLA01j_5f_pta130_NLO_FXFX -nojpeg
